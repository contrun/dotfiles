(setq habitica-uid "{{ secret "habitica" "username" }}")
(setq habitica-token "{{ secret "habitica" "password" }}")

(setq wallabag-host "{{ secret "wallabag" "url" }}")
(setq wallabag-username "{{ secret "wallabag" "username" }}")
(setq wallabag-password "{{ secret "wallabag" "password" }}")
(setq wallabag-clientid "{{ secret "wallabag" "client_id" }}")
(setq wallabag-secret "{{ secret "wallabag" "client_secret" }}")

(defvar my-mu4e-account-alist
  '(("unified"
     (user-mail-address  "{{ secret "unified" "username" }}")
     (user-full-name     "Unified")
     (mu4e-sent-folder   "/unified/Sent")
     (mu4e-drafts-folder "/unified/Drafts")
     (mu4e-trash-folder  "/unified/Deleted")
     (mu4e-refile-folder "/unified/Archive")
     (smtpmail-default-smtp-server "{{ secret "unified" "smtpServer" }}")
     (smtpmail-smtp-user "{{ secret "unified" "username" }}")
     (smtpmail-smtp-server "{{ secret "unified" "smtpServer" }}")
     (smtpmail-stream-type starttls)
     (smtpmail-smtp-service {{ secret "sudo" "smtpPort" }}))
    ("push"
     (user-mail-address  "{{ secret "push" "username" }}")
     (user-full-name     "Push")
     (mu4e-sent-folder   "/push/Sent")
     (mu4e-drafts-folder "/push/Drafts")
     (mu4e-trash-folder  "/push/Deleted")
     (mu4e-refile-folder "/push/Archive")
     (smtpmail-default-smtp-server "{{ secret "push" "smtpServer" }}")
     (smtpmail-smtp-user "{{ secret "push" "username" }}")
     (smtpmail-smtp-server "{{ secret "push" "smtpServer" }}")
     (smtpmail-stream-type starttls)
     (smtpmail-smtp-service {{ secret "push" "smtpPort" }}))
    ("work"
     (user-mail-address  "{{ secret "work" "username" }}")
     (user-full-name     "{{ secret "work" "name" }}")
     (mu4e-sent-folder   "/work/Sent")
     (mu4e-drafts-folder "/work/Drafts")
     (mu4e-trash-folder  "/work/Deleted")
     (mu4e-refile-folder "/work/Archive")
     (smtpmail-default-smtp-server "{{ secret "work" "smtpServer" }}")
     (smtpmail-smtp-user "{{ secret "work" "username" }}")
     (smtpmail-smtp-server "{{ secret "work" "smtpServer" }}")
     (smtpmail-stream-type starttls)
     (smtpmail-smtp-service {{ secret "work" "smtpPort" }}))
    ("sudo"
     (user-mail-address  "{{ secret "sudo" "username" }}")
     (user-full-name     "{{ secret "sudo" "name" }}")
     (mu4e-sent-folder   "/sudo/Sent")
     (mu4e-drafts-folder "/sudo/Drafts")
     (mu4e-trash-folder  "/sudo/Trash")
     (mu4e-refile-folder "/sudo/Archive")
     (smtpmail-default-smtp-server "{{ secret "sudo" "smtpServer" }}")
     (smtpmail-smtp-user "{{ secret "sudo" "username" }}")
     (smtpmail-smtp-server "{{ secret "sudo" "smtpServer" }}")
     (smtpmail-stream-type ssl)
     (smtpmail-smtp-service {{ secret "sudo" "smtpPort" }}))
    )
  )
